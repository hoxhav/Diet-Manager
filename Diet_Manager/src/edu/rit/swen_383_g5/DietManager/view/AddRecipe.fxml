<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.rit.swen_383_g5.DietManager.controller.AddRecipeController">
   <children>
      <AnchorPane id="AnchorPane" layoutY="-3.0" prefHeight="406.0" prefWidth="604.0" style="-fx-background-color: #474747;">
         <children>
            <Button fx:id="addButton" layoutX="235.0" layoutY="212.0" mnemonicParsing="false" onAction="#addRecipe" prefHeight="26.0" prefWidth="74.0" style="-fx-background-color: #42eb85;" text="Add">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <MenuButton fx:id="selectFoodDropdown" layoutX="235.0" layoutY="134.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="155.0" text="Select Food">
               <items>
                  <MenuItem mnemonicParsing="false" text="Action 1" />
                  <MenuItem mnemonicParsing="false" text="Action 2" />
               </items>
            </MenuButton>
            <Label fx:id="amountLabel" layoutX="235.0" layoutY="177.0" text="Amount: " textFill="#42eb85" />
            <TextField fx:id="amount" layoutX="302.0" layoutY="172.0" prefHeight="27.0" prefWidth="87.0" />
            <Button fx:id="addButton1" layoutX="315.0" layoutY="212.0" mnemonicParsing="false" onAction="#remove" prefHeight="26.0" prefWidth="74.0" style="-fx-background-color: #f4427d;" text="Remove" textFill="WHITE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button fx:id="backButton" layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="27.0" prefWidth="56.0" style="-fx-background-color: #f4427d;" text="Back" textFill="WHITE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button fx:id="submitButton" layoutX="235.0" layoutY="245.0" mnemonicParsing="false" onAction="#submit" prefHeight="27.0" prefWidth="150.0" style="-fx-background-color: #42eb85;" text="Submit" textFill="WHITE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
